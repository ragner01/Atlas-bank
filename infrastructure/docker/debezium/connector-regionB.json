{
  "name": "regionB-connector",
  "config": {
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "database.hostname": "postgres_regionB",
    "database.port": "5432",
    "database.user": "postgres",
    "database.password": "postgres",
    "database.dbname": "atlas_b",
    "database.server.name": "regionB",
    "table.include.list": "public.outbox",
    "plugin.name": "pgoutput",
    "slot.name": "debezium_regionB",
    "publication.name": "dbz_publication_regionB",
    "transforms": "route",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "regionB.public.outbox",
    "transforms.route.replacement": "regionB.public.outbox"
  }
}
