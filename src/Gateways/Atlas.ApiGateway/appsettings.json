{
  "ReverseProxy": {
    "Clusters": {
      "ledger": { "Destinations": { "d1": { "Address": "http://ledgerapi:5181" } } },
      "payments": { "Destinations": { "d1": { "Address": "http://paymentsapi:5191" } } }
    },
    "Routes": [
      { "RouteId":"ledger","ClusterId":"ledger","Match": { "Path":"/ledger/{**catchall}" },"Transforms":[{"RequestHeader":"X-Tenant-Id","Set":"{httpContext.Request.Headers.X-Tenant-Id}"}] },
      { "RouteId":"payments","ClusterId":"payments","Match": { "Path":"/payments/{**catchall}" },"Transforms":[{"RequestHeader":"X-Tenant-Id","Set":"{httpContext.Request.Headers.X-Tenant-Id}"}] }
    ]
  }
}
